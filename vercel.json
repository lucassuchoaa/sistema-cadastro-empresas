{
  "version": 2,
  "name": "sistema-cadastro-empresas",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "regions": ["gru1"],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}